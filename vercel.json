{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "\/index.html",
      "dest": "/public/index.html"
    },
    {
      "src": "\/",
      "dest": "/public/index.html"
    },
    {
      "src": "\/api\/(.*)",
      "dest": "api/index.ts"
    },
    {
      "handle": "filesystem"
    }
  ]
}

